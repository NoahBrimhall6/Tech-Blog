{{#each posts}}
<div class="row justify-content-center mb-2">
  <div class="card col col-lg-6">
    <div class="card-body">
      <h5 class="card-title">{{posts.title}}</h5>
      <h6 class="card-subtitle mb-2 text-muted">{{posts.createdAt}}</h6>
      <p class="card-text">{{posts.body}}</p>
    </div>
  </div>
</div>
{{/each}}